with Ada.Text_IO;use Ada.Text_IO;

procedure Addition is

    a: Integer;
    b: Integer;
    c: Integer;
    d: Integer;
    e: Integer;
    f: Integer;
    g: Integer;
    h: Integer;
    i: Integer;
    j: Integer;
    k: Integer;
    y: Integer
    z: Integer

begin
    z:= 5;
    y:= 3;
    a := 3 - 4; --(-1)
    b := 4 - 3; -- 1
    c := 1 - z; --(-4)
    e := z - 1; --4
    f := z - 1 - 1; --3
    h := z - -1; --6
    i := -1 - z; --(-6)
    j := y - z; --(-2)
    k := z - y; --2
end Addition;