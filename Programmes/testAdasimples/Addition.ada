with Ada.Text_IO;use Ada.Text_IO;

procedure Addition is

    a: Integer;
    b: Integer;
    c: Integer;
    d: Integer;
    e: Integer;
    f: Integer;
    g: Integer;
    h: Integer;
    i: Integer;
    j: Integer;
    k: Integer;
    y: Integer
    z: Integer

begin
    z:= 5;
    y:= 3;
    a := 3 + 4; --7
    b := 4 + 3; --7
    c := 1 + z; --6
    e := z + 1; --6
    f := z + 1 - 1; --5
    g := z - 1 + 1; --5
    h := z + -1; --4
    i := -1 + z; --4
    j := y + z; --8
    k := z + y; --8
end Addition;