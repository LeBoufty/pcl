with Ada.Text_IO;use Ada.Text_IO;

procedure Addition is

    a: Integer;
    b: Integer;
    c: Integer;
    d: Integer;
    e: Integer;
    f: Integer;
    g: Integer;
    h: Integer;
    y: Integer
    z: Integer

begin
    z:= 5;
    y:= 3;
    a := 3 / 4; --
    b := 4 / 3; --
    c := 1 / z; --
    d := z / 1; --
    e := z / -1; --
    f := -1 / z; --
    g := y / z; --
    h := z / y; --
end Addition;