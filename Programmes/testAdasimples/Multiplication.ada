with Ada.Text_IO;use Ada.Text_IO;

procedure Addition is

    a: Integer;
    b: Integer;
    c: Integer;
    d: Integer;
    e: Integer;
    f: Integer;
    g: Integer;
    h: Integer;
    y: Integer
    z: Integer

begin
    z:= 5;
    y:= 3;
    a := 3 * 4; --12
    b := 4 * 3; --12
    c := 1 * z; --5
    d := z * 1; --5
    e := z * -1; --(-5)
    f := -1 * z; --(-5)
    g := y * z; --15
    h := z * y; --15
end Addition;